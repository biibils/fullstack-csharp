@model PresensiMVC.Models.Presensi

@{
    ViewBag.Title = "Daftar Presensi";
}

<h1>@ViewBag.Title</h1>

<p>
    <a asp-action="Create" class="btn btn-primary">Tambah Peserta Baru</a>
</p>

<div>
    <h4>@ViewBag.Message</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nama
        </dt>
        <dd class="col-sm-10">
            @Model.Nama
        </dd>
    </dl>
</div>

<p asp-action="Index" class="btn btn-secondary">Kembali ke Daftar Presensi</p>